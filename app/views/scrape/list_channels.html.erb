<h1>Channel data list</h1>
<table border="1" style="border-collapse: collapse">
  <tr>
    <th>ID</th>
    <th>channel name</th>
    <th>label name</th>
    <th># of premium videos</th>
    <th># of public videos</th>
    <th># of label subscribers</th>
    <th># of channel subscribers</th>
    <th># of featured videos</th>
    <th>created_at</th>
    <th>rank</th>
  </tr>
  <% @channels.each do |c| %>
    <tr>
      <td><%= c.id %></td>
      <td><%= c.channel %></td>
      <td><%= c.label   %></td>
      <td><%= c.pre_num %></td>
      <td><%= c.pub_num %></td>
      <td><%= c.l_sub   %></td>
      <td><%= c.c_sub   %></td>
      <td><%= c.feature %></td>
      <td><%= c.created_at.strftime('%Y/%m/%d %H:%M') %></td>
      <td><%= c.rank    %></td>
    </tr>
  <% end %>
</table>
